# Copy this file to .env and fill in your values
# cp .env.example .env

# Flask secret key (generate one: python -c "import secrets; print(secrets.token_hex(32))")
FLASK_SECRET_KEY=change-me-to-a-random-string

# Optional: Gemini API key for AI-powered column mapping
GEMINI_API_KEY=

# Optional: Genius API token for release date enrichment
GENIUS_TOKEN=

# Domain for HTTPS (leave as localhost for local dev, set to your domain for production)
DOMAIN=localhost
